{
  "name": "mirror",
  "version": "1.0.0",
  "description": "Mirror library",
  "main": "dist/index.js",
  "homepage": "https://mirror.ioak.io",
  "scripts": {
    "start": "tsc && webpack serve --env target=development --config webpack.config.js --mode development",
    "start:fast": "webpack serve --env target=development --config webpack.config.js --mode development",
    "build:local": "tsc -w",
    "build": "rollup -c && cp -r ./src/style/ ./dist/",
