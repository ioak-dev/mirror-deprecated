    "deploy": "npm run build&&gh-pages -d build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "graphql:generate": "graphql-codegen --config codegen.yml",
    "rollup": "rollup -c && cp",
    "serve": "serve",
    "copy-css": "cp -r ./src/style/ ./dist/"
  },
